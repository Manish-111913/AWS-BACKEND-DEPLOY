# Lambda Environment Variables Template
# Copy this file to .env.lambda and fill in your actual values

DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require
RUNTIME_DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require
JWT_SECRET=your-jwt-secret-here
FRONTEND_REDIRECT_ORIGIN=https://your-frontend-domain.com
PUBLIC_BASE_URL=https://your-api-gateway-url.amazonaws.com/dev
API_BASE_URL=https://your-api-gateway-url.amazonaws.com/dev
ALLOW_DIRECT_PAY_WITHOUT_ORDER=true
ONBOARDING_TABLE_COUNT=20