# Lambda Environment Variables
# This file shows the environment variables your Lambda function needs
# Set these in AWS Lambda Console or via serverless.yml

NODE_ENV=production
DATABASE_URL=your-production-database-url
DB_HOST=your-db-host
DB_PORT=5432
DB_NAME=qrbilling_prod
DB_USER=your-db-user
DB_PASSWORD=your-db-password
JWT_SECRET=your-secure-jwt-secret
FRONTEND_REDIRECT_ORIGIN=https://your-frontend-domain.com
PUBLIC_BASE_URL=https://your-api-gateway-url.execute-api.ap-south-1.amazonaws.com/dev
ALLOW_DIRECT_PAY_WITHOUT_ORDER=false
ONBOARDING_TABLE_COUNT=20
GOOGLE_VISION_API_KEY=your-google-vision-api-key